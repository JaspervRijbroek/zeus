{
    "name": "zeus",
    "description": "A need for speed world server emulator",
    "dependencies": {
        "debug": "^4.3.1",
        "express": "^4.17.1",
        "glob": "^7.1.6",
        "js2xmlparser": "^4.0.1",
        "jsontoxml": "^1.0.1",
        "mysql2": "^2.2.5",
        "reflect-metadata": "^0.1.13",
        "typeorm": "^0.2.29",
        "xml2json": "^0.12.0"
    },
    "devDependencies": {
        "@types/debug": "^4.1.5",
        "@types/express": "^4.17.9",
        "@types/glob": "^7.1.3",
        "@types/jsontoxml": "^1.0.1",
        "@types/xml2json": "^0.11.3",
        "nodemon": "^2.0.6",
        "ts-node": "^9.1.1",
        "typescript": "^4.1.3"
    },
    "nodemonConfig": {
        "restartable": "rs",
        "ignore": [
            ".git",
            "node_modules"
        ],
        "env": {
            "DEBUG": "nfsw:*"
        },
        "exec": "./node_modules/.bin/ts-node index.ts",
        "watch": [
            "./index.ts",
            "./app/**/*"
        ],
        "ext": "ts"
    }
}
